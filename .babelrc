{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": 8
          },
          "modules": false
        }],
        "stage-0"
      ],
      "plugins": ["istanbul", "rewire"]
    },
    "main": {
      "presets": [
        ["env", {
          "targets": {
            "node": 8
          }
        }],
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "stage-0"
      ]
    }
  },
  "plugins": ["transform-runtime"]
}
